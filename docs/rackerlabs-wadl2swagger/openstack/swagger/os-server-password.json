            "parameters": [
                {
                    "name": "server_id",
                    "required": true,
                    "in": "path",
                    "type": "string",
                    "description": "The UUID for the server of interest to you.\n"
                }
            ],
            "get": {
                "operationId": "getPassword",
                "summary": "Get server password",
                "description": "Gets the administrative password for a specified server.\n",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "200 response",
                        "examples": {
                            "application/json": "{\n    \"password\": \"\"\n}"
                        }
                    }
                }
            },
            "delete": {
                "operationId": "resetPassword",
                "summary": "Clear server password",
                "description": "Clears the encrypted copy of the password in the metadata server. This is done after the client has retrieved the password and knows it doesn't need it in the metadata server anymore. The password for the server remains the same.\n",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "200 response",
                        "examples": {
