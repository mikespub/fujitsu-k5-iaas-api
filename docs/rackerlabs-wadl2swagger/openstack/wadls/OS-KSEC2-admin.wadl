         <method name="GET" rax:id="listCredentials">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="List Credentials">
               <para role="shortdesc">Lists credentials.</para>
            </wadl:doc>
