    <wadl:grammars xmlns:db="http://docbook.org/ns/docbook"
                  xmlns="http://wadl.dev.java.net/2009/02">
      <wadl:include href="14203-os-block-device-mapping-v2-boot-xsd-1.xsd"/>
      <wadl:include href="14203-os-block-device-mapping-v2-boot-xsd-2.xsd"/>
   </wadl:grammars>
    <!--*******************************************************-->
    <!--             All Resources                             -->
