                     </wadl:doc>
                  </param>
                  <param name="security_group_rules"
                         style="plain"
                         required="false"
                         type="xsd:int">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The number of rules allowed for each
                            security group.</para>
                     </wadl:doc>
                  </param>
                  <param name="security_groups"
                         style="plain"
                         required="true"
                         type="xsd:int">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The number of security groups allowed
                            for each tenant.</para>
                     </wadl:doc>
                  </param> 
               </representation>
            </response>
         </method>
