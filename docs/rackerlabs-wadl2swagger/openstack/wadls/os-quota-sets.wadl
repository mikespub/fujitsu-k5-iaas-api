                         style="plain"
                         required="true"
                         type="xsd:string">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>A <code>quota_set</code> object.</para>
                     </wadl:doc>
                  </param>
                  <param name="cores" style="plain" required="true" type="xsd:int">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The number of instance cores allowed for
                            each tenant.</para>
                     </wadl:doc>
                  </param>
                  <param name="fixed_ips " style="plain" required="true" type="xsd:int">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
