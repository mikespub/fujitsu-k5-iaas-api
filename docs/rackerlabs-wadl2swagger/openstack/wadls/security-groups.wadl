         <method name="POST" rax:id="createSecGroup">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Create security group">
               <para role="shortdesc">Creates an OpenStack Networking
                security group.</para>
               <para>This operation creates a security group with default
                security group rules for the IPv4 and IPv6 ether
                types.</para>
            </wadl:doc>
            <request>
               <param name="name" type="xsd:string" required="true">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>A
                        symbolic name for the security group. Not
                        required to be unique.</para>
                  </wadl:doc>
               </param>
               <param name="description" type="xsd:string" required="false">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Describes the security
                    group.</para>
                  </wadl:doc>
               </param>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "security_group": {
        "name": "new-webservers",
        "description": "security group for webservers"
    }
}</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
               </representation>
            </request>
            <response status="201">
               <param name="security_group" type="xsd:string" required="true">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Security group
                    object.</para>
                  </wadl:doc>
               </param>
               <param name="description" type="xsd:string" required="true">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The
                        security group description.</para>
                  </wadl:doc>
               </param>
               <param name="id" type="csapi:uuid" required="true">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The
                        UUID for the security group.</para>
                  </wadl:doc>
               </param>
               <param name="name" type="xsd:string" required="true">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The
                        security group name.</para>
                  </wadl:doc>
